pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo " I am in build stage of uat2"
            }
        }
        stage('Test') { 
            steps {
                echo " I am in Test stage of uat2"
            }
        }
        stage('Deploy') { 
            steps {
                echo " I am in Deploy stage of uat2"
            }
        }
        stage('Archive') { 
            steps {
                echo " I am in Archive stage of uat2"
            }
        }
    }
}
