pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo " I am in build stage of dev2"
            }
        }
        stage('Test') { 
            steps {
                echo " I am in Test stage dev2"
            }
        }
        stage('Deploy') { 
            steps {
                echo " I am in Deploy stage dev2"
            }
        }
        stage('Archive') { 
            steps {
                echo " I am in Archive stage dev2"
            }
        }
    }
}
